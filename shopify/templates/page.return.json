      }
    },
    "1658222605618c1f10": {
      "type": "list",
      "blocks": {
        "f8457f0d-ed8c-417d-8aef-11ba6c5df9dd": {
          "type": "item",
          "settings": {
            "text": "Return shipping is non-refundable, unless you receive damaged, defective or wrong item(s)."
          }
        },
        "ff70b90c-e794-4f91-95de-17efeb3c0a24": {
          "type": "item",
          "settings": {
            "text": "Refunds on discounted or sale items will be refunded at the prorated amount"
          }
        },
        "05679c5f-176f-455d-a131-7f88cc7b3e5a": {
          "type": "item",
          "settings": {
            "text": "Refunds must be processed through your original payment method, otherwise they will be issued in store credit (gift card)."
          }
        },
        "45fd5816-1563-4f35-8d52-0aef4c64589b": {
          "type": "item",
          "settings": {
            "text": "Original shipping charges are non-refundable"
          }
        },
        "ed240e1e-cb3a-4655-b5a8-d53bdb566c55": {
          "type": "item",
          "settings": {
            "text": "Any orders sent with \"Free Shipping\" will be subject to the original shipping fee deducted from the total amount of the refund."
          }
        },
        "69c1f45a-e383-4956-8ff4-1d8039458e7e": {
          "type": "item",
          "settings": {
            "text": "International Returns - unfortunately we cannot refund duties, taxes or shipping charges"
          }
        }
      },
      "block_order": [
        "f8457f0d-ed8c-417d-8aef-11ba6c5df9dd",
        "ff70b90c-e794-4f91-95de-17efeb3c0a24",
