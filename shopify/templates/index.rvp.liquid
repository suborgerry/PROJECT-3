{%- comment -%}Please DO NOT edit this file{%- endcomment -%}
        {%- layout none -%}
        {%- assign rvpParamKey = 'q' -%}
        {%- assign rvpParamValue = '' -%}
        {%- capture contentForQuerystring -%}{{ content_for_header }}{%- endcapture -%}
        {%- assign rvpPageUrl = contentForQuerystring | split:'"pageurl":"' | last | split:'"' | first | split:'.myshopify.com' | last |
           replace:'\/','/' |
           replace:'%20',' ' |
           replace:'\u0026','&'
        -%}
        {%- assign pageQuerystring = rvpPageUrl | split:'?' | last -%}
        {%- assign rvpParts = pageQuerystring | split:'&' -%}
        {%- for rvpPart in rvpParts -%}
          {%- assign rvpKeyAndValue = rvpPart | split:'=' -%}
          {%- if rvpKeyAndValue[0] == rvpParamKey -%}
            {%- assign rvpParamValue = rvpKeyAndValue[1] | strip -%}
          {%- endif -%}
        {%- endfor -%}
        {%- assign namespace = 'td_' | append: rvpParamValue -%}
        {
          "metafields": {
            "shop": {{ shop.permanent_domain | json }},
            "settings": {{ shop.metafields[namespace].app_settings }},
            "moneyFormat": {{ shop.money_format | json }}
          }
        }