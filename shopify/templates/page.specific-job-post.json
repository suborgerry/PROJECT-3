    "165870486219ffe53e": {
      "type": "list",
      "blocks": {
        "4fd3ddf5-9b15-4261-971a-b557c051da52": {
          "type": "item",
          "settings": {
            "text": "Must be positive, hard-working, multitasking, enthusiastic and energetic"
          }
        },
        "df7916d6-9804-402c-8800-e41ed3dd9c12": {
          "type": "item",
          "settings": {
            "text": "Detail-oriented: must be able to quickly assemble customer orders with 100% accuracy."
          }
        },
        "e87f382c-8f7a-4d17-9ad6-c0411c9249bf": {
          "type": "item",
          "settings": {
            "text": "Dependable and able to work both unsupervised and as part of a team"
          }
        },
        "971f9273-f7e2-4591-b1ce-1b69750bc18b": {
          "type": "item",
          "settings": {
            "text": "Must be able to stand\/walk, lift boxes, climb a ladder and move around a warehouse"
          }
        },
        "05747cd3-db65-49b2-8c18-b6e7bb015213": {
          "type": "item",
          "settings": {
            "text": "Must thrive in a fast-paced, results-oriented environment"
          }
        },
        "7c22ab41-526f-40e0-9a09-d47af008ad0e": {
          "type": "item",
          "settings": {
            "text": "E-commerce, inventory, customer service and shipping experience preferred."
          }
        }
      },
