        "1658704137689a8c41-0": {
          "type": "heading",
          "settings": {
            "heading": "Job Summary",
            "margin_top": 0,
            "margin_top_mob": 0
          }
        },
        "1658704137689a8c41-1": {
          "type": "text",
          "settings": {
            "text": "<p>Route One Apparel is seeking a part- or full-time fulfillment and warehouse associate responsible for completing all customer orders efficiently and accurately according to online orders. Other responsibilities consist of inventory management, customer service and closing duties such as preparing the center for the next business day. On-the-job training is provided.<\/p>",
            "text_center": false,
            "text_size": 14,
            "text_size_mobile": 16
          }
        }
      },
      "block_order": [
        "1658704137689a8c41-0",
        "1658704137689a8c41-1"
      ],
      "settings": {
        "padding_top": 40,
        "padding_bottom": 8
      }
    },
    "165870450685b5ebf1": {
      "type": "list",
      "blocks": {
        "dc3b1a12-de5a-4e0c-a873-c30b33024992": {
          "type": "item",
          "settings": {
            "text": "Work with Packing Team to accurately pack and fulfill all online orders correctly and quickly.Ability to pass packing tests and increase your packing efficiency levels"
          }
        },
        "f167f683-e05a-489b-a6a3-c52e3e4487e8": {
          "type": "item",
          "settings": {
            "text": "Work inside the warehouse, selecting, packing, wrapping, inspecting and shipping products and packages for shipment in an efficient, error free manner.Learn from mistakes."
          }
        },
        "fbdaf79f-9221-42e6-96aa-3924755cd25f": {
          "type": "item",
          "settings": {
            "text": "Organizing, checking in and stocking merchandise"
          }
        },
        "c6a7f490-3730-4a43-b611-67ac4a43007e": {
          "type": "item",
          "settings": {
            "text": "Putting together boxes for shipping"
          }
        },
        "44b317ad-24cf-451f-a66d-939e55cce9b7": {
          "type": "item",
          "settings": {
            "text": "Processing returns by putting merchandise back in stock"
          }
        },
        "b2b5b7d8-5568-4021-b880-3e33500a555a": {
          "type": "item",
          "settings": {
            "text": "Counting merchandise when handling inventory control"
          }
        },
        "b6eb5eba-8420-4873-80b2-c5eb92fb614f": {
          "type": "item",
          "settings": {
            "text": "Perform basic housekeeping in assigned areas of the warehouse"
          }
        },
        "1754e81c-6f70-40e5-a856-3483752eba3b": {
          "type": "item",
          "settings": {
            "text": "Adhere to all safety procedures in the performance of job duties"
          }
        }
      },
