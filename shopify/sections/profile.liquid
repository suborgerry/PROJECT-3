                                        </select>
                                        <svg aria-hidden="true" focusable="false" viewBox="0 0 10 6">
                                            <use href="#icon-caret" />
                                        </svg>
                                        </div>
                                    </div>
                                    <h4 class="field__title">{{ 'customer.addresses.zip' | t | replace: '/', ' or ' }}</h4>
                                    <div class="field">
                                        <input class="field__input check-visited-input" type="text" id="AddressZipNew" name="address[zip]" value="{{ form.zip }}" autocapitalize="characters" autocomplete="postal-code" placeholder="{{ 'customer.addresses.zip' | t }}">
                                        <label for="AddressZipNew">{{ 'customer.addresses.zip' | t }}</label>
                                    </div>
                                    <div>
                                        {{ form.set_as_default_checkbox }}
                                        <label for="address_default_address_new">{{ 'customer.addresses.set_default' | t }}</label>
                                    </div>
                                    <div>
                                        <button class="button__red">{{ 'customer.addresses.add' | t }}</button>
                                        <button class="button__cancel" type="reset">{{ 'customer.addresses.cancel' | t }}</button>
                                    </div>
                                    {%- endform -%}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="profile__column">
                    <h3 class="text-24px-all">{{ 'Wishlist' | upcase }}</h3>
                    <div class="profile__wishlist profile__block-style">
                        <div id="wishlist-items-container"></div>
                    </div>
                </div>
        </div>
    </div>
</section>

<script>
    window.onload = () => {
      typeof CustomerAddresses !== 'undefined' && new CustomerAddresses();
    }
</script>

